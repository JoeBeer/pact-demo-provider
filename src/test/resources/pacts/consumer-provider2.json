{
  "consumer": {
    "name": "consumer"
  },
  "interactions": [
    {
      "description": "A request for a greeting message",
      "providerStates": [
        {
          "name": "default"
        }
      ],
      "request": {
        "method": "GET",
        "path": "/greeting"
      },
      "response": {
        "body": {
          "message": "Hello, World!"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "status": 200
      }
    }
  ],
  "metadata": {
    "pact-jvm": {
      "version": "4.5.10"
    },
    "pactSpecification": {
      "version": "3.0.0"
    }
  },
  "provider": {
    "name": "provider"
  }
}
